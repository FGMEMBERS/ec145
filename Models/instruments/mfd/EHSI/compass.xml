        </layer>

        <layer>
            <texture>
                <path>ehsi.png</path>
                <x1>0.046</x1>
                <y1>0.051</y1>
                <x2>0.117</x2>
                <y2>0.121</y2>
            </texture>
            <w>38</w>
            <h>38</h>
            <emissive>true</emissive>
            <transformations>
                <transformation>
                    <type>y-shift</type>
                    <offset>-122.0</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <texture>
                <path>arc.png</path>
                <x1>0.011</x1>
                <y1>0.216</y1>
                <x2>0.100</x2>
                <y2>0.229</y2>
            </texture>
            <w>28</w>
            <h>6</h>
            <emissive>true</emissive>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>366.0</offset>
                </transformation>
                <transformation>
                    <type>rotation</type>
                    <property>autopilot/internal/vert-speed-fpm</property>
                    <interpolation>
                        <entry><ind>-4000.0</ind><dep>-36.0</dep></entry>
                        <entry><ind>-3000.0</ind><dep>-29.5</dep></entry>
                        <entry><ind>-2000.0</ind><dep>-22.0</dep></entry>
                        <entry><ind>-1000.0</ind><dep>-14.5</dep></entry>
                        <entry><ind>0.0</ind><dep>0.0</dep></entry>
                        <entry><ind>1000.0</ind><dep>14.5</dep></entry>
                        <entry><ind>2000.0</ind><dep>22.0</dep></entry>
                        <entry><ind>3000.0</ind><dep>29.5</dep></entry>
                        <entry><ind>4000.0</ind><dep>36.0</dep></entry>
                    </interpolation>
                </transformation>
                <transformation>
                    <type>x-shift</type>
                    <offset>-147.0</offset>
                </transformation>
            </transformations>
        </layer>

    <layer>
        <name>HDG display</name>
        <type>text</type>
        <point-size>20</point-size>
        <font>helvetica_bold</font>
        <color>
            <red>0</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </color>
        <chunks>
            <chunk>
                <type>number-value</type>
                <property>autopilot/settings/heading-bug-deg</property>
                <format>%03.0f</format>
            </chunk>
        </chunks>
        <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>120.0</offset>
            </transformation>
            <transformation>
                <type>x-shift</type>
                <offset>184.0</offset>
            </transformation>
        </transformations>
   </layer>

    <layer>
        <name>CRS display</name>
        <type>text</type>
        <point-size>20</point-size>
        <font>helvetica_bold</font>
        <color>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </color>
        <chunks>
            <chunk>
                <type>number-value</type>
                <property>instrumentation/nav/radials/selected-deg</property>
                <format>%03.0f</format>
            </chunk>
        </chunks>
        <transformations>
            <transformation>
                <type>y-shift</type>
                <offset>-162.0</offset>
            </transformation>
            <transformation>
                <type>x-shift</type>
                <offset>-244.0</offset>
            </transformation>
        </transformations>
   </layer>

</layers>

</PropertyList>