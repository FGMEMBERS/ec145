<?xml version="1.0"?>
<PropertyList>

    <name>hsi compass</name>
    <w-base>512</w-base>
    <h-base>366</h-base>

    <layers>

        <layer>
            <texture>
                <path>ehsi.png</path>
                <x1>0.110</x1>
                <y1>0.208</y1>
                <x2>0.892</x2>
                <y2>0.989</y2>
            </texture>
            <w>524</w>
            <h>524</h>
            <emissive>true</emissive>
            <transformations>
                <transformation>
                    <type>y-shift</type>
                    <offset>-122.0</offset>
                </transformation>
                <transformation>
                    <type>rotation</type>
                    <property>orientation/heading-magnetic-deg</property>
                    <max>360</max>
                    <scale>-1</scale>
                </transformation>
            </transformations>
        </layer>


        <layer>
            <texture>
                <path>ehsi.png</path>
                <x1>0.282</x1>
                <y1>0.014</y1>
                <x2>0.719</x2>
                <y2>0.172</y2>
            </texture>
            <w>424</w>
            <h>153</h>
            <emissive>true</emissive>
            <transformations>
                <transformation>
                    <type>y-shift</type>
                    <offset>-122.0</offset>
                </transformation>
                <transformation>
                    <type>rotation</type>
                    <property>orientation/heading-magnetic-deg</property>
                    <max>360</max>
                    <scale>-1</scale>
                </transformation>
                <transformation>
                    <type>rotation</type>
                    <offset>90.0</offset>
                    <property>instrumentation/nav/radials/selected-deg</property>
                    <max>360</max>
                    <scale>1</scale>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <texture>
                <path>ehsi.png</path>
                <x1>0.622</x1>
                <y1>0.088</y1>
                <x2>0.815</x2>
                <y2>0.098</y2>
            </texture>
            <w>239</w>
            <h>9</h>
            <emissive>true</emissive>
            <transformations>
                <transformation>
                    <type>y-shift</type>
                    <offset>-122.0</offset>
                </transformation>
                <transformation>
                    <type>rotation</type>
                    <property>orientation/heading-magnetic-deg</property>
                    <max>360</max>
                    <scale>-1</scale>
                </transformation>
                <transformation>
                    <type>rotation</type>
                    <offset>90.0</offset>
                    <property>instrumentation/nav/radials/selected-deg</property>
                    <max>360</max>
                    <scale>1</scale>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <property>instrumentation/nav/heading-needle-deflection-norm</property>
                    <scale>67</scale>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <texture>
                <path>ehsi.png</path>
                <x1>0.945</x1>
                <y1>0.152</y1>
                <x2>0.988</x2>
                <y2>0.170</y2>
            </texture>
            <w>26</w>
            <h>10</h>
            <emissive>true</emissive>
            <transformations>
                <transformation>
                    <type>y-shift</type>
                    <offset>-122.0</offset>
                </transformation>
                <transformation>
                    <type>rotation</type>
                    <property>autopilot/internal/heading-bug-error-deg</property>
                    <max>360</max>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>240.0</offset>
                </transformation>
            </transformations>
        </layer>


        <layer>
            <texture>
                <path>arc.png</path>
                <x1>0.001</x1>
                <y1>0.285</y1>
                <x2>1.0</x2>
                <y2>0.997</y2>
            </texture>
            <w>512</w>
            <h>366</h>
